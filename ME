#NiceHexSpiral.py
import turtle
Colors=['red', 'purple', 'blue',
           'green', 'yellow', 'orange']
T=tutrle.Pen()
turtle.bgcolor('black')
for x in range (360):
    t.pencolor(colors[x%6])
    t.width(x/100+1)
    t.forward(x)
    t.left(59)
